# === Dropbox (shared data sync) ===
DROPBOX_APP_KEY=
DROPBOX_APP_SECRET=
DROPBOX_REFRESH_TOKEN=

# === Volcengine 火山引擎 (Chat LLM) ===
VOLCENGINE_API_KEY=
VOLCENGINE_BASE_URL=https://ark.cn-beijing.volces.com/api/coding/v3

# === AIHUBMIX (Embedding) ===
# Volcengine does not support embedding; AIHUBMIX provides text-embedding-3-small
AIHUBMIX_API_KEY=
AIHUBMIX_BASE_URL=https://aihubmix.com/v1

# === RD-Agent Settings ===
# Chat model (via LiteLLM, mapped in litellm_config.yaml)
CHAT_MODEL=volcengine/glm-4.7
# Embedding model (via LiteLLM → AIHUBMIX)
EMBEDDING_MODEL=aihubmix/text-embedding-3-small

# === Paths (WSL2 native paths for production) ===
QLIB_DATA_PATH=~/qlib_rd_agent/data/qlib
RDAGENT_WORKSPACE=~/qlib_rd_agent/workspace
